pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
-- melda ‚

debug = false
player = {}
player.x = 64
player.y = 64
player.moving = false
player.direction = 3
player.sprite = 0
player.attacking = 0

cam = {}
cam.x = 0
cam.y = 0

atk_btn_down = false

ani_frame = 0

num_enemies = 4
enemies = {}

function _init()
	--music(0, 1000)
	
	for i = 0, num_enemies do
 	enemies[i] = {}
 	enemies[i].x = 32
 	enemies[i].y = (8 * i) + 8
 	enemies[i].sprites = {}
 	enemies[i].sprites[0] = 32
 	enemies[i].sprites[1] = 33
 	enemies[i].tick_off = flr(rnd(20))
 end
end

function _update()
 ani_frame = ani_frame + 1
	if ani_frame >= 20 then
		ani_frame = 0
	end
	
	player.attacking = player.attacking - 1
	if player.attacking < 0 then
		player.attacking = 0
	end

 player.moving = false
	if player.attacking == 0 and btn(0) then
	 new_x_ish = ((player.x - 1) / 8) * 8
	 s1 = is_solid_at(new_x_ish, player.y)	 
	 s2 = is_solid_at(new_x_ish, player.y+7)
	 
	 if not s1 and not s2 then
			player.x = player.x - 1
			player.moving = true
		end
		player.direction = 0
	end
	
	if player.attacking == 0 and btn(1) then
		new_x_ish = ((player.x + 8) / 8) * 8
	 s1 = is_solid_at(new_x_ish, player.y)
		s2 = is_solid_at(new_x_ish, player.y + 7)
	
	 if not s1 and not s2 then
			player.x = player.x + 1
			player.moving = true
		end
		player.direction = 1
	end
	
	if player.attacking == 0 and btn(2) then
		new_y_ish = ((player.y - 1) / 8) * 8
	 s1 = is_solid_at(player.x, new_y_ish)
	 s2 = is_solid_at(player.x + 7, new_y_ish)

		if not s1 and not s2 then
			player.y = player.y - 1
			player.moving = true
		end
		player.direction = 2
	end
	
	if player.attacking == 0 and btn(3) then
		new_y_ish = ((player.y + 8) / 8) * 8
	 s1 = is_solid_at(player.x, new_y_ish)
	 s2 = is_solid_at(player.x + 7, new_y_ish)

		if not s1 and not s2 then
			player.y = player.y + 1
			player.moving = true
		end
		player.direction = 3
	end
		
	if btn(4) then
		sfx(0)
	end

	if btn(5) and
				not atk_btn_down and 
				player.attacking == 0 then
		sfx(1)
		player.attacking = 10
		atk_btn_down = true
	end
	
	if not btn(5) then
		atk_btn_down = false
	end
	
	cam.x = player.x - 64
	cam.y = player.y - 64
	
	if cam.x < 0 then cam.x = 0 end
	if cam.y < 0 then cam.y = 0 end
end

function get_tile_at(x, y)
	tile_x = x / 8
	tile_y = y / 8
	
	return mget(tile_x, tile_y)
end

function is_solid_at(x, y)
	return is_solid(get_tile_at(x,y))
end

function is_solid(tile_num)
	return fget(tile_num, 1)
end

function _draw()
 cls()
 
	local sword_xoffset = 0
	local sword_yoffset = 0
	
	if player.direction == 0 then
		sword_xoffset = -8
		if player.moving then
	 	if ani_frame < 10 then
 			player.sprite = 2
 		else
 			player.sprite = 1
 		end
 	elseif player.attacking > 0 then
 		player.sprite = 12
 	else
 		player.sprite = 0
 	end
 elseif player.direction == 1 then
 	sword_xoffset = 8
 	if player.moving then
	 	if ani_frame < 10 then
 			player.sprite = 5
 		else
 			player.sprite = 4
 		end
		elseif player.attacking > 0 then
			player.sprite = 13
 	else
 		player.sprite = 3
 	end
 elseif player.direction == 2 then
  sword_yoffset = -8
 	if player.moving then
	  if ani_frame < 10 then
	 		player.sprite = 8
	 	else
	 		player.sprite = 7
	 	end
 	elseif player.attacking > 0 then
 		player.sprite = 14
	 else
	 	player.sprite = 6
	 end
 else 
 	sword_yoffset = 8
  if player.moving then
	  if ani_frame < 10 then
	 		player.sprite = 11
	 	else
	 		player.sprite = 10
	 	end
	 elseif player.attacking > 0 then
 		player.sprite = 15
	 else
	 	player.sprite = 9
	 end
 end
 
 --map(0,0, 0,20, 32,32)
	map(0,0, 0,0, 32,32)
	
	for	i = 0,num_enemies do
		local sprt_idx = 0
		local tick = (ani_frame + enemies[i].tick_off) % 20
	 if tick <= 10 then
	 	sprt_idx = 1
	 end
	 local sprite = enemies[i].sprites[sprt_idx]
		spr(sprite, enemies[i].x, enemies[i].y)
	end
	
 spr(player.sprite, player.x, player.y)

	if player.attacking > 0 then
		spr(16 + player.direction, player.x + sword_xoffset, player.y + sword_yoffset)
	end

	camera(cam.x, cam.y)
	
	if debug then
		print("x "..player.x.." y "..player.y,0 + cam.x,0 + cam.y,7)
		print("")
	end
end
